@inproceedings{10.1145/2971763.2971780,
author = {Brending, Stefan and Khan, Ali Mahmood and Lawo, Michael and M\"{u}ller, Maik and Zeising, Patrick},
title = {Reducing anxiety while interacting with industrial robots},
year = {2016},
isbn = {9781450344609},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/2971763.2971780},
doi = {10.1145/2971763.2971780},
abstract = {Studies in the field of human-robot collaboration have shown that the direct cooperation of humans and robots can lead to increased anxiety feelings of workers. Previous studies realize either a collaboration with lightweight robots or a temporal and spatial separation of humans and robots. We use a robot with a load capacity greater than 200 kg with a temporal and spatial overlap of the working areas. Three different prototypes for Google Glass render the current state of the system in the form of text, icons or a traffic light. The evaluation in a comparative field study shows that when using any of the three prototypes, the perceived state anxiety is low.},
booktitle = {Proceedings of the 2016 ACM International Symposium on Wearable Computers},
pages = {54–55},
numpages = {2},
keywords = {trust, human-robot collaboration, head mounted displays},
location = {Heidelberg, Germany},
series = {ISWC '16}
}

@article{10.1145/210079.210088,
author = {Greenhalgh, Chris and Benford, Steven},
title = {MASSIVE: a collaborative virtual environment for teleconferencing},
year = {1995},
issue_date = {Sept. 1995},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {2},
number = {3},
issn = {1073-0516},
url = {https://doi.org/10.1145/210079.210088},
doi = {10.1145/210079.210088},
abstract = {We describe a prototype virtual reality teleconferencing system called MASSIVE which has been developed as part of our on-going research into collaborative virtual environments. This system allows multiple users to communicate using arbitrary combinations of audio, graphics, and text media over local and wide area networks. Communication is controlled by a so-called spatial model of interaction so that one user's perception of another user is sensitive to their relative positions and orientations. The key concept in this spatial model is the (quantitative) awareness which one object has of another. This is controlled by the observing object's focus and the observed object's nimbus, which describe regions of interest and projection, respectively. Each object's aura defines the total region within which it interacts. This is applied independently in each medium. The system (and the spatial model which it implements) is intended to provide a flexible and natural environment for the spatial mediation of conversation. The model also provides a basis for scaling to relatively large numbers of users. Our design goals include supporting heterogeneity, scalability, spatial mediation, balance of power, and multiple concurrent meetings; MASSIVE meets all of these goals. Our initial experiences show the importance of audio in collaborative VR, and they raise issues about field of view for graphical users, speed of navigation, quality of embodiment, varying perceptions of space, and scalability.},
journal = {ACM Trans. Comput.-Hum. Interact.},
month = {sep},
pages = {239–261},
numpages = {23},
keywords = {scalability, CSCW}
}

@inproceedings{10.1145/3054977.3054987,
author = {Kim, Ji Eun and Fan, Xiangmin and Mosse, Daniel},
title = {Empowering End Users for Social Internet of Things},
year = {2017},
isbn = {9781450349666},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3054977.3054987},
doi = {10.1145/3054977.3054987},
abstract = {We present Socialite, a novel end user programming tool for the Social Internet of Things (SIoT). SIoT is a new paradigm where IoT merges with social networks, allowing people and connected devices as well as the devices themselves to interact within a social network framework. Through an online survey with 60 potential users, we identified eight desired features for the SIoT, which were then clustered into four rule categories that can be programmed by end users and/or imposed by systems. The rules created by end users are used to reason about both devices and people in their social relationships to support automated decisions during runtime. Socialite uses ontology/semantic models for basic/low-level knowledge representation (e.g., device and user) to encapsulate the heterogeneity in devices from various manufacturers, and uses production rules (trigger-action programming) for high-level reasoning. With the ontology model, our reasoning supports both device type automation (e.g., current temperature from a thermostat) and capability-based automation (e.g., current temperature from any devices with the same capability). Furthermore, the Socialite rules leverage social relationships and device capabilities to facilitate collaboration by efficiently sharing configuration and information among users/friends and even with devices from people unknown to a user. In a 24-participant user study (12 with no programming experience), we found that Socialite was easy to learn and use, for both programmers and non-programmers. Participants were able to create automation based rules, social relationship involved rules, as well rules they created during the study.},
booktitle = {Proceedings of the Second International Conference on Internet-of-Things Design and Implementation},
pages = {71–82},
numpages = {12},
keywords = {Social Internet of Things, Semantic Reasoning Framework, End User Programming},
location = {Pittsburgh, PA, USA},
series = {IoTDI '17}
}

@inproceedings{10.5555/3635637.3663302,
author = {Berger, Cyrille and Doherty, Patrick and Rudol, Piotr and Wzorek, Mariusz},
title = {A Summary of the RGS⊕: an RDF Graph Synchronization System for Collaborative Robotics},
year = {2024},
isbn = {9798400704864},
publisher = {International Foundation for Autonomous Agents and Multiagent Systems},
address = {Richland, SC},
abstract = {In the context of collaborative robotics, distributed situation awareness is essential for supporting collective intelligence in teams of robots and human agents, where it can be used for both individual and collective decision support. This is particularly important in applications pertaining to emergency rescue and crisis management. During operational missions, data and knowledge is gathered incrementally and in different ways by heterogeneous robots and humans. This paper aims to describe an RDF Graph Synchronization System called RGS⊕. It is assumed that a dynamic set of agents provide or retrieve knowledge stored in their local RDF Graphs which are continuously synchronized between agents. The RGS⊕ System was designed to handle unreliable communication and does not depend on a static centralized infrastructure.},
booktitle = {Proceedings of the 23rd International Conference on Autonomous Agents and Multiagent Systems},
pages = {2827–2829},
numpages = {3},
keywords = {multi-robot collaboration, rdf graph synchronization},
location = {<conf-loc>, <city>Auckland</city>, <country>New Zealand</country>, </conf-loc>},
series = {AAMAS '24}
}

@inproceedings{10.1145/3534678.3539237,
author = {Cui, Sen and Liang, Jian and Pan, Weishen and Chen, Kun and Zhang, Changshui and Wang, Fei},
title = {Collaboration Equilibrium in Federated Learning},
year = {2022},
isbn = {9781450393850},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3534678.3539237},
doi = {10.1145/3534678.3539237},
abstract = {Federated learning (FL) refers to the paradigm of learning models over a collaborative research network involving multiple clients without sacrificing privacy. Recently, there have been rising concerns on the distributional discrepancies across different clients, which could even cause counterproductive consequences when collaborating with others. While it is not necessarily that collaborating with all clients will achieve the best performance, in this paper, we study a rational collaboration called "collaboration equilibrium'' (CE), where smaller collaboration coalitions are formed. Each client collaborates with certain members who maximally improve the model learning and isolates the others who make little contribution. We propose the concept of benefit graph which describes how each client can benefit from collaborating with other clients and advance a Pareto optimization approach to identify the optimal collaborators. Then we theoretically prove that we can reach a CE from the benefit graph through an iterative graph operation. Our framework provides a new way of setting up collaborations in a research network. Experiments on both synthetic and real world data sets are provided to demonstrate the effectiveness of our method.},
booktitle = {Proceedings of the 28th ACM SIGKDD Conference on Knowledge Discovery and Data Mining},
pages = {241–251},
numpages = {11},
keywords = {federated learning, collaborative learning, collaboration equilibrium},
location = {Washington DC, USA},
series = {KDD '22}
}

@proceedings{10.1145/1543137,
title = {MUM '08: Proceedings of the 7th International Conference on Mobile and Ubiquitous Multimedia},
year = {2008},
isbn = {9781605581927},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
abstract = {We are proud to welcome you all to MUM2008, the 7th international conference on Mobile and Ubiquitous Multimedia ! (www.mum2008.org/ )Multimedia is becoming ubiquitous and we already now and/or soon will be able access any multimedia content, from anywhere in the world, on any device. This conference addresses this theme and looks at the current cutting-edge technologies enabling mobile and ubiquitous multimedia. MUM 2008 targets novel and forward looking research that focuses on the architectures, protocols, and algorithms to cope with mobility. MUM2008 is also targeting case studies, field trials, and evaluation of new applications and services, as well as research in novel mobile user interfaces, research in design and use of intelligent, aware, proactive, and attentive environments. MUM 2008 is also a conference focused on research in middleware and distributed-computing support for mobile and ubiquitous multimedia, mobile computer graphics, games, and entertainment, mobile and ubiquitous multimedia in intelligent transportation systems. Finally, MUM2008 is also focusing on research in novel adaptive, context-aware, and wireless multimedia applications and systems, mobile media management and mobile and ubiquitous multimedia in ad hoc networks. Overall, MUM2008 is about the trends, major challenges, and perspectives related to mobile and ubiquitous multimedia.This year the MUM conference is organized in collaboration between Ume\r{a} university and Lule\r{a} University of Technology, and in cooperation (In-Coop) with ACM SigMobile at Ume\r{a} university, 3--5 Dec 2008, Ume\r{a}, Sweden.MUM 2008 is a leading annual international conference, which provides a forum for presenting the latest research results on mobile and ubiquitous multimedia, and previous conference venues include: Oulu, FIN (2007), Stanford, USA (2006), Christchurch, NZ (2005), College Park, USA (2004), Norrk\"{o}ping, SWE (2003) and Oulu, FIN (2002).The conference's call-for-papers resulted in forty five submissions from seventeen countries. These were subject to a rigorous review procedure. Every paper had three or four independent reviews. Twenty regular papers from both academia and industry were selected for publication in this volume of proceedings making the selection rate 42%. Regular papers were allocated up to 8 pages in ACM double column format each. Further selection process identified seven short papers for publication (up to 4 pages each) and presentation based on their work-in-progress. Then a separate process has identified 6 papers with industry focus. The presentations of the selected papers are organized in nine sessions as reflected in the table of content:•Ubiquitous interaction and ubiquitous multimedia•Smart multimedia applications and services•Sharing and management of multimedia in heterogeneous networks•Context-aware multimedia applications and services•User-centric mobile multimedia•Wireless multimedia networking•Mobile and ubiquitous applications --- 1•Mobile and ubiquitous applications --- 2•Industrial paper sessionWhile keeping the single-track tradition of the technical program of the MUM conferences, MUM2008 program includes two tutorials during morning of the first day of the conference. These tutorials are given by experts in the field and present state-of-the-art achievement. In particular, Prof Timo Ojala from the University of Oulu will present "Theories and case studies on mobile interactive system design and evaluation", while Prof Kostas Pentikousis will present "Mobility and Multiaccess in Emerging Internet Architectures".This year the MUM2008 conference has two distinguished keynote speakers. First keynote speaker comes from telecommunications industry --- Mr Martin K\"{o}rling, Director, Service layer research, Ericsson Research. A second keynote speaker comes from academia --- Prof. Rikard Lindgren, University of Gothenburg. Such a balance of industry and academia is indicative of how the area of mobile and ubiquitous multimedia spans across academic research as well as extending to commercial applications and industrial R&amp;D. Another indication of industry support is the sponsorship that we have received for the organization of MUM'08 from Ericsson and Nokia, two leading international companies in Scandinavia with strong interest and involvement in digital technologies for enabling mobile and ubiquitous multimedia.We hope this conference will steer exciting and fruitful discussions, new ideas, new collaborations, and new ways of thinking about, and approaching the field of mobile and ubiquitous multimedia.},
location = {Ume\r{a}, Sweden}
}

